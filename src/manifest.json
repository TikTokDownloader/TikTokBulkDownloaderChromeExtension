{
  "manifest_version": 3,
  "name": "Bulk video downloader for TikTok | TikTokDer",
  "short_name": "TikTokDer",
  "description": "Video download + bulk user video downloader",
  "version": "1.0.0",
  "icons": {
    "32": "./icon.png",
    "128": "./icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/@*"],
      "js": ["./background.js"]
    }
  ],
  "homepage_url": "https://tiktokder.com",
  "host_permissions": [
    "https://www.tiktok.com/",
    "*://*/*"
  ]
}
